    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "babel-preset-expo": "^12.0.4",
    "date-fns": "^2.28.0",
    "dotenv": "^16.4.7",
