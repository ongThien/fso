    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.11.10",
    "@expo/metro-config": "^0.17.4",
    "@expo/metro-runtime": "~3.1.1",
