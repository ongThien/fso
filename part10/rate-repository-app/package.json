    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "eslint-plugin-jest": "^28.9.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.2",
    "metro-react-native-babel-preset": "^0.77.0",
