{
  "name": "rate-repository-app",
  "main": "index",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start -c",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
